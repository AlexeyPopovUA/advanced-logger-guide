    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "workbox-webpack-plugin": "6.0.2"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "100 kB"
    }
