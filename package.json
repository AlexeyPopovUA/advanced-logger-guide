{
  "name": "advanced-logger-guide",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://alexeypopovua.github.io/advanced-logger-guide",
  "dependencies": {
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router-dom": "^4.3.1",
    "prismjs": "latest"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest",
    "coverage": "npm test -- --coverage --no-cache",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.3",
    "@types/prismjs": "^1.9.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/node_modules/"
    ],
    "testRegex": "__tests__/.*\\.test\\.tsx$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.ts"
    }
  }
}
