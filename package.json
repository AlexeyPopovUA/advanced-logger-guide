{
  "name": "advanced-logger-guide",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://alexeypopovua.github.io/advanced-logger-guide",
  "dependencies": {
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router-dom": "^4.3.1",
    "prismjs": "latest"
  },
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage --no-cache",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-dev": "webpack --config webpack.config.js  --env.dev",
    "dev": "webpack-dev-server --config webpack.config.js  --env.dev --env.watch",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.3",
    "@types/prismjs": "^1.9.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/node_modules/"
    ],
    "testRegex": "__tests__/.*\\.test\\.tsx$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.ts"
    }
  }
}
