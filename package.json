{
  "name": "advanced-logger-guide",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://alexeypopovua.github.io/advanced-logger-guide",
  "dependencies": {
    "bulma": "0.9.1",
